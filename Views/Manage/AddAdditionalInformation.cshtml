@model AddAdditionalViewModel
@{
    ViewData["Title"] = "Profile";
    ViewData.AddActivePage(ManageNavPages.Index);
}

<h4>@ViewData["Title"]</h4>
<div class="row">
    <div class="col-md-6">
        <form method="post">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="GitHubUrl"></label>
                <input asp-for="GitHubUrl" class="form-control" disabled />
                <span asp-validation-for="GitHubUrl" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TwitterUrl"></label>
                <input asp-for="TwitterUrl" class="form-control" disabled />
                <span asp-validation-for="TwitterUrl" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="VkontakteUrl"></label>
                <input asp-for="VkontakteUrl" class="form-control" disabled />
                <span asp-validation-for="VkontakteUrl" class="text-danger"></span>
            </div>
            <button type="submit" class="btn btn-default">Save</button>
        </form>
    </div>
</div>

@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}
